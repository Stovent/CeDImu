	nam CSD
	ttl CSD of SoftCDI
	use c:/DEFS/OSKDEFS.D

typelang	equ	(CSDData<<8)
attrrevs	equ	(ReEnt<<8)+$99
edition		equ	0
stacksize	equ 0
entrypt		equ csd

	psect csd,typelang,attrrevs,edition,stacksize,entrypt

dataoffset dc.l $34 Data files must have this offset.
csd	dc.b	"1:/cd:",$D
	dc.b	"3:/vid:LI=\"525\":TV:",$D
	dc.b	"4:/nvr:",$D

* Align to 16 bytes size
	rept (16-(*-csd))&15
	dc.b	0
	endr
	ends
