	nam nvdrv
	use c:\DEFS\OSKDEFS.D

typelang	equ	(Drivr<<8)+Objct
attrrevs	equ	((ReEnt+SupStat)<<8)+$99 Not reentrant
edition		equ	0
stacksize	equ	0
entrypt		equ	EntryTable

	psect nvdrv,typelang,attrrevs,edition,stacksize,entrypt

EntryTable	dc.w	Init
		dc.w	Read
		dc.w	Write
		dc.w	GetStat
		dc.w	SetStat
		dc.w	Term
		dc.w	0	Trap handler

Init
	eor.w d0,d0
	rts

Read
	trap #8
	eor.w d0,d0
	rts

Write
	trap #9
	eor.w d0,d0
	rts

GetStat
	trap #10
	eor.w d0,d0
	rts

SetStat
	trap #11
	eor.w d0,d0
	rts

Term
	eor.w d0,d0
	rts

	vsect
	ds.w 0x50
	ends

	ends
