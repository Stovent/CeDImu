	nam ciapdriv
	use c:\DEFS\OSKDEFS.D
	use softcdi.d
	use softcdi.m

typelang	equ	(Drivr<<8)+Objct
attrrevs	equ	((ReEnt+SupStat)<<8)+$99 Not reentrant
edition		equ	0
stacksize	equ	0
entrypt		equ	EntryTable

	psect ciapdriv,typelang,attrrevs,edition,stacksize,entrypt

EntryTable	dc.w	Init
		dc.w	Read
		dc.w	Write
		dc.w	GetStat
		dc.w	SetStat
		dc.w	Term
		dc.w	0	Trap handler

Init
	softcdi $101
	rts

Read
	softcdi $102
	rts

Write
	softcdi $103
	rts

GetStat
	softcdi Ucm_GetStat
	rts

SetStat
	softcdi Ucm_SetStat
	rts

Term
	softcdi $106
	rts

	vsect
	ds.w 0x50
	ends

	ends
